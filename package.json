{
  "name": "ecotopiajs",
  "version": "0.0.1",
  "description": "EcotopiaJS helps monitor and optimize software power consumption by tracking CPU, memory, debugging, and CI/CD pipeline energy usage for eco-friendly development.",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "keywords": [
    "ecotopia",
    "ecotopiajs",
    "power-monitoring",
    "energy-tracking",
    "eco-friendly",
    "sustainability",
    "cpu-usage",
    "memory-usage",
    "ci-cd",
    "typescript",
    "performance",
    "javascript-library",
    "npm-package",
    "typescript",
    "typescript-library",
    "typescript-support"
  ],
  "author": "M. Ergin Turk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RecursiveVoid/ecotopiajs.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.9.0",
    "prettier": "^3.4.2",
    "rollup": "^4.27.4",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {},
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  }
}
